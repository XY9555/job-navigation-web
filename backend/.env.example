# 环境配置示例文件
# 复制此文件为 .env 并填入实际值

# 运行环境
NODE_ENV=development

# 服务器端口
PORT=3000

# 数据库配置（生产环境）
DATABASE_URL=postgresql://username:password@host:port/database

# AI服务配置
ZHIPU_API_KEY=your_zhipu_api_key_here

# JWT密钥
JWT_SECRET=your_jwt_secret_here

# 文件上传配置
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads